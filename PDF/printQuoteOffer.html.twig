<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//FR"
        "http://www.w3.org/TR/html4/strict.dtd">
<html>
<head>
    <title>PDF Paprec Offer</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
    <style type="text/css">
        html,
        body {
            margin: 0;
            padding: 0;
            font-family: "Red Hat Display", sans-serif;
            font-weight: 500;
            font-size: 14px;
            color: #4a4a4a;
        }

        * {
            box-sizing: border-box;
            -webkit-box-sizing: border-box;
        }

        h1 {
            color: #004a8e;
            font-size: 20px;
            font-weight: 900;
            margin: 0;
            width: 70%;
        }

        h2 {
            font-size: 14px;
            font-weight: bold;
            margin-bottom: 15px;
            margin-top: 20px;
            border-bottom: 1px solid #4a4a4a;
            padding-bottom: 5px;
        }

        p {
            margin: 5px 0;
            padding-bottom: 15px;
        }

        hr {
            border: 1px dashed #d8d8d8;
            width: 200px;
            height: 1px;
            margin: 8px 20px 0 20px;
        }

        .bold {
            font-weight: bold;
        }

        .text-left {
            text-align: left;
        }

        .text-right {
            text-align: right;
        }

        .offer__container {
            width: 800px;
            /*height: 1280px;*/
            margin: auto;
            /*margin-bottom: 50px;*/
            position: relative;
        }

        /* FLEXBOX */

        .flex {
            display: -webkit-box;
            display: -webkit-flex;
            display: -ms-flexbox;
            display: -moz-box;
            /*display: box;*/
            display: flex;
        }

        .align-item-center {
            -webkit-box-align: center;
            -moz-box-align: center;
            -ms-flex-align: center;
            -webkit-align-items: center;
            box-align: center;
            align-items: center;
        }

        .align-item-end {
            -webkit-box-align: end;
            -moz-box-align: end;
            -ms-flex-align: end;
            -webkit-align-items: end;
            box-align: end;
            align-items: flex-end;
        }

        .justify-center {
            -webkit-box-pack: center;
            box-pack: center;
            justify-content: center;
        }

        .justify-end {
            -webkit-box-pack: end;
            box-pack: end;
            justify-content: flex-end;
        }

        .space-between {
            -webkit-box-pack: justify;
            box-pack: justify;
            justify-content: space-between;
        }

        .w50 {
            width: 50%;
        }

        .mb-14 {
            margin-bottom: 14px;
        }

        .mb-20 {
            margin-bottom: 20px;
        }

        .mb-30 {
            margin-bottom: 30px;
        }

        .mb-40 {
            margin-bottom: 40px;
        }

        /***** PAGE OFFRE *****/

        /*Title*/

        .offer__title {
            border-bottom: 2px solid #004a8e;
        }

        .offer__title__ref {
            /*border-top: 2px solid #004a8e;*/
            /*border-right: 2px solid #004a8e;*/
            /*border-left: 2px solid #004a8e;*/
            margin: 0;
            padding: 2px 5px 0 5px;
            color: #f00f0f;
            font-size: 20px;
            text-transform: uppercase;
            font-weight: 900;
        }

        .offer__title__date {
            /*border-top: 2px solid #004a8e;*/
            /*border-right: 2px solid #004a8e;*/
            /*border-left: 2px solid #004a8e;*/
            color: #004a8e;
            margin: 0;
            padding: 2px 5px 0 5px;
            font-size: 20px;
            text-transform: uppercase;
            font-weight: 900;
        }

        /*content*/

        .offer__content {
            padding: 0 0 30px 0;
        }

        .offer__ref-items > div:first-child {
            width: 50%;
        }

        .offer__ref-items > div:last-child {
            width: 50%;
            margin-left: 70px;
            padding-right: 15px;
        }

        table {
            border-collapse: separate;
            border-spacing: 0;
            width: 90%;
            margin-bottom: 20px;
        }

        table p {
            margin: 0;
        }

        table td, table th {
            font-size: 13px;
            overflow: hidden;
            word-break: normal;
            text-align: center;
            border: 1px solid #004a8e;
            border-right: 0px;
            border-bottom: 0px;
            height: 22px;
        }

        table th {
            padding: 5px 10px;
            vertical-align: top;
        }

        table td {
            padding: 2px 5px;
        }

        table span {
            font-weight: normal;
        }

        table tr:last-child > td {
            border-bottom: 1px solid #004a8e;
        }

        table tr > td:last-child, table tr > th:last-child {
            border-right: 1px solid #004a8e;
        }

        table .table-header td {
            padding: 0;
            border-top: 0;
        }

        table .table-footer1 td:nth-child(n+2):nth-child(-n+3) {
            border-left: 0;
        }

        table .table-footer1 td:last-child {
            border-left: 0;
        }

        table .table-footer1 td:nth-child(n+5):nth-child(-n+7) {
            border-bottom: 1px solid #004a8e;
        }

        table td[rowspan="2"] {
            border-bottom: 1px solid #004a8e;
        }

        table .table-footer1 td {
            padding: 2px 0;
        }

        table .table-footer2 td:nth-child(n+2):nth-child(-n+3) {
            border-left: 0;
        }

        table .table-footer2 td:nth-child(-n+4) {
            border-top: 0;
        }

        table .table-footer2 td:last-child {
            border-right: 0;
        }

        table .table-footer2 td {
            padding: 0 0 2px 0;
        }

        table .empty {
            height: 15px;
        }

        table .empty td:last-child {
            border-left: 0 !important;
            border-right: 0 !important;
        }

        .check {
            display: inline-block;
            width: 10px;
            height: 10px;
            outline: 2px solid grey;
            margin-left: 5px;
        }

        .check.active {
            border: 2px solid white;
            outline: 2px solid grey;
            background-color: black;
        }

        .offer__signature > div {
            width: 48%;
        }

        /* simple-table */

        table.simple-table tr:not(:first-child) td {
            border-top: 0 !important;
        }

        table.simple-table td {
            padding: 2px 5px;
        }

        table.simple-table tr td:first-child {
            width: 250px;
            padding-right: 30px;
        }

        table.simple-table tr td:nth-child(2) {
            width: 150px;
            border-left: 0;
        }

        table.simple-table tr td:nth-child(3) {
            width: 100px;
        }

        .cgv-page {
            page-break-before: always;
        }

        .other {
            page-break-inside: avoid;
        }

        table td.line-number {
            text-align: right;
        }

    </style>

</head>
<body>
<div class="offer__container">

    <div class="offer__title flex space-between">
        <h1>{{ 'Commercial.PDF.Title'|trans({'%number%': quoteRequest.duration|default(0)}, "messages", app.request.locale) }}
            - {{ quoteRequest.reference }}</h1>
        <p class="offer__title__date">{{ 'now'|date('d/m/Y') }}</p>
    </div>

    <div class="offer__content">

        <div class="offer__ref">

            {# partie 1 #}

            <h2>1. {{ 'Commercial.PDF.CustomerTitle'|trans({}, "messages", app.request.locale) }}</h2>
            <p>
                <span class="bold">{{ 'Commercial.PDF.Company'|trans({}, "messages", app.request.locale) }} :</span> {{ quoteRequest.businessName }}
            </p>
            <p>
                <span class="bold">{{ 'Commercial.PDF.Address'|trans({}, "messages", app.request.locale) }} :</span> {{ quoteRequest.address }}
            </p>
            <p>
                <span class="bold">{{ 'Commercial.PDF.PostalCode'|trans({}, "messages", app.request.locale) }} :</span> {% if quoteRequest.postalCode %}{{ quoteRequest.postalCode.code }}{% endif %} {{ quoteRequest.city }}
            </p>

            {# partie 2 #}

            <h2>2. {{ 'Commercial.PDF.ContactTitle'|trans({}, "messages", app.request.locale) }}</h2>

            <div class="flex offer__ref-items">
                <div>
                    <p>
                        <span class="bold">{{ 'Commercial.PDF.SignatoryLastName'|trans({}, "messages", app.request.locale) }} :</span> {{ quoteRequest.signatoryLastName1 }}
                    </p>
                </div>

                <div>
                    <p>
                        <span class="bold">{{ 'Commercial.PDF.ContactLastName'|trans({}, "messages", app.request.locale) }} :</span> {{ quoteRequest.lastName }}
                    </p>
                </div>
            </div>

            <div class="flex offer__ref-items">
                <div>
                    <p>
                        <span class="bold">{{ 'Commercial.PDF.SignatoryFirstName'|trans({}, "messages", app.request.locale) }} :</span> {{ quoteRequest.signatoryFirstName1 }}
                    </p>
                </div>

                <div>
                    <p>
                        <span class="bold">{{ 'Commercial.PDF.ContactFirstName'|trans({}, "messages", app.request.locale) }} :</span> {{ quoteRequest.firstName }}
                    </p>
                </div>
            </div>

            <div class="flex offer__ref-items">
                <div>
                </div>

                <div>
                    <p>
                        <span class="bold">{{ 'Commercial.PDF.PhoneNumber'|trans({}, "messages", app.request.locale) }} :</span> {{ quoteRequest.phone }}
                    </p>
                </div>
            </div>

            <div class="flex offer__ref-items">
                <div></div>

                <div>
                    <p>
                        <span class="bold">{{ 'Commercial.PDF.Email'|trans({}, "messages", app.request.locale) }} :</span> {{ quoteRequest.email }}
                    </p>
                </div>
            </div>

            {# partie 3 #}

            <h2>3. {{ 'Commercial.PDF.ServiceTitle'|trans({}, "messages", app.request.locale) }}</h2>

            <table class="table table-striped table-bordered table-hover"
                   cellpadding="0" cellspacing="0" border="0" width="100%">
                <thead>
                <tr>
                    <th>{{ 'Commercial.QuoteRequestLine.PDF.ProductCode'|trans }}</th>
                    <th>{{ 'Commercial.QuoteRequestLine.ProductRange'|trans }}</th>
                    <th>{{ 'Commercial.QuoteRequestLine.ProductName'|trans }}</th>
                    <th>{{ 'Commercial.QuoteRequestLine.ProductBillingUnit'|trans }}</th>
                    <th>{{ 'Commercial.QuoteRequestLine.PDF.Quantity'|trans }}</th>
                    <th>{{ 'Commercial.QuoteRequestLine.PDF.PassingFrequency'|trans }}</th>
                    <th>{{ 'Commercial.QuoteRequestLine.RentalPrice'|trans }}</th>
                    <th>{{ 'Commercial.QuoteRequestLine.TreatmentCollect'|trans }}</th>
                    <th>{{ 'Commercial.QuoteRequestLine.Treatment'|trans }} </th>
                    <th>{{ 'Commercial.QuoteRequestLine.Package'|trans }} </th>
                </tr>
                </thead>
                <tbody>
                {% for quoteRequestLine in quoteRequest.quoteRequestLines %}
                    <tr>
                        <td>
                            {{ quoteRequestLine.product.code }}
                        </td>
                        <td>
                            {% if quoteRequestLine.product.range %}
                                {{ quoteRequestLine.product.range|rangeLabelTranslation(app.user.lang) }}
                            {% endif %}
                        </td>
                        <td>
                            {{ quoteRequestLine.product|productLabelTranslation(app.user.lang) }}
                        </td>
                        <td>
                            {% if quoteRequestLine.product.billingUnit %}
                                {{ quoteRequestLine.product.billingUnit.name }}
                            {% endif %}
                        </td>
                        <td class="line-number">
                            {% if quoteRequestLine.product.calculationFormula == 'PACKAGE' %}
                                {{ quoteRequestLine.quantity }}
                            {% elseif quoteRequestLine.product.calculationFormula == 'UNIT_PRICE' %}
                                {# RESTE NULL #}
                            {% else %}
                                {{ quoteRequestLine.quantity }}
                            {% endif %}
                        </td>
                        <td>
                            {% if quoteRequestLine.product.hideFrequency == 1 %}

                            {% else %}
                                {% if quoteRequestLine.frequency != 'REGULAR' %}
                                    {% if quoteRequestLine.frequency %}
                                        {{ ('General.Frequency.'~quoteRequestLine.frequency)|trans({}, "messages", app.request.locale) }}
                                    {% endif %}
                                {% else %}
                                    {% if quoteRequestLine.frequencyInterval is not empty %}
                                        {% set frequencyIntervalName = 'General.Frequency.' ~ quoteRequestLine.frequencyInterval %}
                                        {{ quoteRequestLine.frequencyTimes }}
                                        x/ {{ frequencyIntervalName|trans({}, "messages", app.request.locale) }}
                                    {% endif %}
                                {% endif %}
                            {% endif %}
                        </td>
                        <td class="line-number">
                            {% if quoteRequestLine.product.calculationFormula == 'PACKAGE' %}
                                {{ 0|formatAmount('EUR', app.request.locale) }}
                            {% elseif quoteRequestLine.product.calculationFormula == 'UNIT_PRICE' %}
                                {{ (quoteRequestLine|quoteRequestLineCalculatePrices('editableRentalUnitPrice'))|formatAmount('EUR', app.request.locale) }}
                            {% else %}
                                {{ (quoteRequestLine|quoteRequestLineCalculatePrices('editableRentalUnitPrice'))|formatAmount('EUR', app.request.locale) }}
                            {% endif %}
                        </td>
                        <td class="line-number">
                            {% if quoteRequestLine.product.calculationFormula == 'PACKAGE' %}
                                {{ 0|formatAmount('EUR', app.request.locale) }}
                            {% elseif quoteRequestLine.product.calculationFormula == 'UNIT_PRICE' %}
                                {{ (quoteRequestLine|quoteRequestLineCalculatePrices('treatmentCollectPrice'))|formatAmount('EUR', app.request.locale) }}
                            {% else %}
                                {{ (quoteRequestLine|quoteRequestLineCalculatePrices('treatmentCollectPrice'))|formatAmount('EUR', app.request.locale) }}
                            {% endif %}
                        </td>
                        <td class="line-number">
                            {% if quoteRequestLine.editableTreatmentUnitPrice < 0 %}
                                <i class="fas fa-exclamation-triangle" data-toggle="tooltip"
                                   data-placement="top"
                                   title="Le prix sera indexé mensuellement à la variation de la mercuriale Mercuriale choisie, référence Mois et Année de Référence"></i>
                            {% endif %}
                            {% if quoteRequestLine.product.calculationFormula == 'PACKAGE' %}
                                {{ 0|formatAmount('EUR', app.request.locale) }}
                            {% elseif quoteRequestLine.product.calculationFormula == 'UNIT_PRICE' %}
                                {{ (quoteRequestLine|quoteRequestLineCalculatePrices('editableTreatmentUnitPrice'))|formatAmount('EUR', app.request.locale) }}
                            {% else %}
                                {{ (quoteRequestLine|quoteRequestLineCalculatePrices('editableTreatmentUnitPrice'))|formatAmount('EUR', app.request.locale) }}
                            {% endif %}
                        </td>
                        <td class="line-number">
                            {% if quoteRequestLine.product.calculationFormula == 'PACKAGE' or quoteRequestLine.product.catalog == 'MATERIAL' %}
                                {{ (quoteRequestLine|quoteRequestLineCalculatePrices('totalAmount'))|formatAmount('EUR', app.request.locale) }}
                            {% endif %}
                        </td>
                    </tr>
                {% endfor %}
                </tbody>
            </table>
            {% if quoteRequest.managementFeeCode1 is not empty or quoteRequest.managementFeeDescription1 is not empty or quoteRequest.managementFeeFrequency1 is not empty or (quoteRequest.managementFeeAmount1 is not null and quoteRequest.managementFeeAmount1 >= 0) or quoteRequest.managementFeeCode2 is not empty or quoteRequest.managementFeeDescription2 is not empty or quoteRequest.managementFeeFrequency2 is not empty or (quoteRequest.managementFeeAmount2 is not null and quoteRequest.managementFeeAmount2 >= 0) or quoteRequest.managementFeeCode3 is not empty or quoteRequest.managementFeeDescription3 is not empty or quoteRequest.managementFeeFrequency3 is not empty or (quoteRequest.managementFeeAmount3 is not null and quoteRequest.managementFeeAmount3 >= 0) or quoteRequest.managementFeeCode4 is not empty or quoteRequest.managementFeeDescription4 is not empty or quoteRequest.managementFeeFrequency4 is not empty or (quoteRequest.managementFeeAmount4 is not null and quoteRequest.managementFeeAmount4 >= 0) %}
                <table class="table table-striped table-bordered table-hover"
                       cellpadding="0" cellspacing="0" border="0" width="100%">
                    <thead>
                    <tr>
                        <th>{{ 'Commercial.QuoteRequest.ManagementFeeCode'|trans }}</th>
                        <th>{{ 'Commercial.QuoteRequest.ManagementFeeDescription'|trans }}</th>
                        <th>{{ 'Commercial.QuoteRequest.ManagementFeeFrequency'|trans }}</th>
                        <th>{{ 'Commercial.QuoteRequest.ManagementFeeAmount'|trans }}</th>
                    </tr>
                    </thead>
                    <tbody>
                    {% if quoteRequest.managementFeeCode1 is not empty or quoteRequest.managementFeeDescription1 is not empty or quoteRequest.managementFeeFrequency1 is not empty or (quoteRequest.managementFeeAmount1 is not null and quoteRequest.managementFeeAmount1 >= 0) %}
                        <tr>
                            <td>{{ quoteRequest.managementFeeCode1 }}</td>
                            <td>{{ quoteRequest.managementFeeDescription1 }}</td>
                            <td>{% if quoteRequest.managementFeeFrequency1 %}{{ ('General.Frequency.' ~ quoteRequest.managementFeeFrequency1)|trans({}, "messages", app.request.locale) }}{% endif %}</td>
                            <td>{{ quoteRequest.managementFeeAmount1|formatAmount('EUR', app.request.locale) }}</td>
                        </tr>
                    {% endif %}
                    {% if quoteRequest.managementFeeCode2 is not empty or quoteRequest.managementFeeDescription2 is not empty or quoteRequest.managementFeeFrequency2 is not empty or (quoteRequest.managementFeeAmount2 is not null and quoteRequest.managementFeeAmount2 >= 0) %}
                        <tr>
                            <td>{{ quoteRequest.managementFeeCode2 }}</td>
                            <td>{{ quoteRequest.managementFeeDescription2 }}</td>
                            <td>{% if quoteRequest.managementFeeFrequency2 %}{{ ('General.Frequency.' ~ quoteRequest.managementFeeFrequency2)|trans({}, "messages", app.request.locale) }}{% endif %}</td>
                            <td>{{ quoteRequest.managementFeeAmount2|formatAmount('EUR', app.request.locale) }}</td>
                        </tr>
                    {% endif %}
                    {% if quoteRequest.managementFeeCode3 is not empty or quoteRequest.managementFeeDescription3 is not empty or quoteRequest.managementFeeFrequency3 is not empty or (quoteRequest.managementFeeAmount3 is not null and quoteRequest.managementFeeAmount3 >= 0) %}
                        <tr>
                            <td>{{ quoteRequest.managementFeeCode3 }}</td>
                            <td>{{ quoteRequest.managementFeeDescription3 }}</td>
                            <td>{% if quoteRequest.managementFeeFrequency3 %}{{ ('General.Frequency.' ~ quoteRequest.managementFeeFrequency3)|trans({}, "messages", app.request.locale) }}{% endif %}</td>
                            <td>{{ quoteRequest.managementFeeAmount3|formatAmount('EUR', app.request.locale) }}</td>
                        </tr>
                    {% endif %}
                    {% if quoteRequest.managementFeeCode4 is not empty or quoteRequest.managementFeeDescription4 is not empty or quoteRequest.managementFeeFrequency4 is not empty or (quoteRequest.managementFeeAmount4 is not null and quoteRequest.managementFeeAmount4 >= 0) %}
                        <tr>
                            <td>{{ quoteRequest.managementFeeCode4 }}</td>
                            <td>{{ quoteRequest.managementFeeDescription4 }}</td>
                            <td>{% if quoteRequest.managementFeeFrequency4 %}{{ ('General.Frequency.' ~ quoteRequest.managementFeeFrequency4)|trans({}, "messages", app.request.locale) }}{% endif %}</td>
                            <td>{{ quoteRequest.managementFeeAmount4|formatAmount('EUR', app.request.locale) }}</td>
                        </tr>
                    {% endif %}


                    </tbody>
                </table>
            {% endif %}
            <div>
                <span class="bold">{{ 'Commercial.PDF.OurIntervention'|trans({}, "messages", app.request.locale) }} :</span>
                <div>
                    <ul>
                        <li>Mise en place du matériel sous 72h</li>
                        <li>Collecte des déchets stockés dans les contenants manipulés par votre personnel</li>
                        <li>Traitement et recyclage de vos déchets sur nos sites Paprec, agréés et certifiés</li>
                        <li>Envoi d’un reporting mensuel</li>
                    </ul>
                </div>
            </div>


            {% if quoteRequest.catalog == 'REGULAR' or quoteRequest.catalog == 'regular' %}
                <div>
                    <span class="bold">{{ 'Commercial.PDF.ContractDuration'|trans({}, "messages", app.request.locale) }} :</span>
                    <div style="margin-top: 5px">
                        {% if quoteRequest.duration is not null %}
                            {{ quoteRequest.duration }} {{ 'Commercial.QuoteRequest.MONTH'|trans }} à compter de la date de signature du présent contrat, ou de la date spécifiée au point 4.
                        {% endif %}
                    </div>
                    {% if quoteRequest.startDate is not null %}
                        <div>
                            La date de démarrage des prestations est prévue pour
                            le {{ quoteRequest.startDate|localizeddate('short', 'none', app.request.locale, app.user.timezone) }}
                            .
                        </div>
                    {% endif %}
                </div>
            {% elseif quoteRequest.catalog == 'PONCTUAL' or quoteRequest.catalog == 'ponctual' or quoteRequest.catalog == 'MATERIAL' or quoteRequest.catalog == 'material' %}
                <div>
                    <span class="bold">{{ 'Commercial.PDF.PonctualDate'|trans({}, "messages", app.request.locale) }} :</span>
                    <div style="margin-top: 5px">

                        {% if quoteRequest.ponctualDate is not null %}
                            {{ quoteRequest.ponctualDate|localizeddate('short', 'none', app.request.locale, app.user.timezone) }}
                        {% endif %}
                    </div>
                </div>
            {% endif %}

        </div>


        {# partie 4 #}

        <div>
            <h2>4. {{ 'Commercial.PDF.CommentTitle'|trans({}, "messages", app.request.locale) }}</h2>
            <div>
                {% if quoteRequest.comment %}
                    {{ quoteRequest.comment }}
                {% else %}
                    {{ 'Commercial.PDF.NoComment'|trans({}, "messages", app.request.locale) }}
                {% endif %}
            </div>
        </div>


        {# partie 5 #}

        <div class="other">

            <h2>5. {{ 'Commercial.PDF.OtherContractualTitle'|trans({}, "messages", app.request.locale) }}</h2>

            <div class="flex offer__ref-items">
                <div>
                    <p>
                    <span class="bold"
                          style="margin-left: 150px">{{ 'Commercial.PDF.BillingSubTitle'|trans({}, "messages", app.request.locale) }}</span>
                    </p>
                </div>

                <div>
                    <p>
                        <span class="bold">{{ 'Commercial.PDF.SiteSubTitle'|trans({}, "messages", app.request.locale) }} </span>
                    </p>
                </div>
            </div>

            <div class="flex offer__ref-items">
                <div class="flex">
                    <p style="width: 150px">{{ 'Commercial.PDF.CorporateName'|trans({}, "messages", app.request.locale) }}</p>
                    <input type="text"
                           style="width: 280px; height: 30px; background-color: #F2F2F2; border: 1px solid black;">
                </div>

                <div class="flex">
                    <input type="text"
                           style="width: 280px; height: 30px; background-color: #F2F2F2; border: 1px solid black;"
                           value="{{ quoteRequest.businessName }}">
                </div>
            </div>

            <div class="flex offer__ref-items" style="margin-bottom: 10px;">
                <div class="flex">
                    <p style="width: 150px">{{ 'Commercial.PDF.Address'|trans({}, "messages", app.request.locale) }}</p>
                    <input type="text"
                           style="width: 280px; height: 60px; background-color: #F2F2F2; border: 1px solid black;"
                           {% if quoteRequest.isSameAddress %}value="{{ quoteRequest.address }}"
                           {% else %}value="{{ quoteRequest.billingAddress }}"{% endif %}>
                </div>

                <div class="flex">
                    <input type="text"
                           style="width: 280px; height: 60px; background-color: #F2F2F2; border: 1px solid black;"
                           value="{{ quoteRequest.address }}">
                </div>
            </div>

            <div class="flex offer__ref-items">
                <div class="flex">
                    <p style="width: 150px">{{ 'Commercial.PDF.City'|trans({}, "messages", app.request.locale) }}</p>
                    <input type="text"
                           style="width: 280px; height: 30px; background-color: #F2F2F2; border: 1px solid black;"
                           {% if quoteRequest.isSameAddress %}value="{{ quoteRequest.city }}"
                           {% else %}value="{{ quoteRequest.billingCity }}"{% endif %}>
                </div>

                <div class="flex">
                    <input type="text"
                           style="width: 280px; height: 30px; background-color: #F2F2F2; border: 1px solid black;"
                           value="{{ quoteRequest.city }}">
                </div>
            </div>

            <div class="flex offer__ref-items">
                <div class="flex">
                    <p style="width: 150px">{{ 'Commercial.PDF.PhoneNumber'|trans({}, "messages", app.request.locale) }}</p>
                    <input type="text"
                           style="width: 280px; height: 30px; background-color: #F2F2F2; border: 1px solid black;">
                </div>

                <div class="flex">
                    <input type="text"
                           style="width: 280px; height: 30px; background-color: #F2F2F2; border: 1px solid black;"
                           value="{{ quoteRequest.phone }}">
                </div>
            </div>

            <div class="flex offer__ref-items">
                <div class="flex">
