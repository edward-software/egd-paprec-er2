<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//FR"
        "http://www.w3.org/TR/html4/strict.dtd">
<html>
<head>
    <title>PDF Paprec Offer</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
    <style type="text/css">
        html,
        body {
            margin: 0;
            padding: 0;
            font-family: "Red Hat Display", sans-serif;
            font-weight: 500;
            font-size: 17px;
            color: #4a4a4a;
        }

        * {
            box-sizing: border-box;
            -webkit-box-sizing: border-box;
        }

        h1 {
            color: #004a8e;
            font-size: 20px;
            font-weight: 900;
            margin: 0;
            width: 70%;
        }

        h2 {
            font-size: 17px;
            font-weight: bold;
            margin-bottom: 15px;
            margin-top: 20px;
            /*border-bottom: 1px solid #4a4a4a;*/
            padding-bottom: 5px;
        }

        p {
            margin: 5px 0;
            padding-bottom: 15px;
        }

        hr {
            border: 1px dashed #d8d8d8;
            width: 200px;
            height: 1px;
            margin: 8px 20px 0 20px;
        }

        .bold {
            font-weight: bold;
        }

        .text-left {
            text-align: left;
        }

        .text-right {
            text-align: right;
        }

        .offer__container {
            width: 800px;
            /*height: 1280px;*/
            margin: auto;
            /*margin-bottom: 50px;*/
            position: relative;
        }

        /* FLEXBOX */

        .flex {
            display: -webkit-box;
            display: -webkit-flex;
            display: -ms-flexbox;
            display: -moz-box;
            /*display: box;*/
            display: flex;
        }

        .align-item-center {
            -webkit-box-align: center;
            -moz-box-align: center;
            -ms-flex-align: center;
            -webkit-align-items: center;
            box-align: center;
            align-items: center;
        }

        .align-item-end {
            -webkit-box-align: end;
            -moz-box-align: end;
            -ms-flex-align: end;
            -webkit-align-items: end;
            box-align: end;
            align-items: flex-end;
        }

        .justify-center {
            -webkit-box-pack: center;
            box-pack: center;
            justify-content: center;
        }

        .justify-end {
            -webkit-box-pack: end;
            box-pack: end;
            justify-content: flex-end;
        }

        .space-between {
            -webkit-box-pack: justify;
            box-pack: justify;
            justify-content: space-between;
        }

        .w50 {
            width: 50%;
        }

        .mb-14 {
            margin-bottom: 14px;
        }

        .mb-20 {
            margin-bottom: 20px;
        }

        .mb-30 {
            margin-bottom: 30px;
        }

        .mb-40 {
            margin-bottom: 40px;
        }

        /***** PAGE OFFRE *****/

        /*Title*/

        .offer__title {
            border-bottom: 2px solid #004a8e;
        }

        .offer__title__ref {
            /*border-top: 2px solid #004a8e;*/
            /*border-right: 2px solid #004a8e;*/
            /*border-left: 2px solid #004a8e;*/
            margin: 0;
            padding: 2px 5px 0 5px;
            color: #f00f0f;
            font-size: 20px;
            text-transform: uppercase;
            font-weight: 900;
        }

        .offer__title__date {
            /*border-top: 2px solid #004a8e;*/
            /*border-right: 2px solid #004a8e;*/
            /*border-left: 2px solid #004a8e;*/
            color: #004a8e;
            margin: 0;
            padding: 2px 5px 0 5px;
            font-size: 20px;
            text-transform: uppercase;
            font-weight: 900;
        }

        /*content*/

        .offer__content {
            padding: 0 0 30px 0;
        }

        .offer__ref-items > div:first-child {
            width: 50%;
        }

        .offer__ref-items > div:last-child {
            width: 50%;
            margin-left: 70px;
            padding-right: 15px;
        }

        table {
            border-collapse: separate;
            border-spacing: 0;
            margin-bottom: 20px;
        }

        table p {
            margin: 0;
        }

        table td, table th {
            font-size: 16px;
            overflow: hidden;
            word-break: normal;
            text-align: center;
            border: 1px solid #004a8e;
            border-right: 0px;
            border-top: 0px;
            height: 22px;
        }

        table thead th {
            border-top: 1px solid #004a8e;
        }

        table th {
            padding: 5px 10px;
            vertical-align: top;
        }

        table td {
            padding: 2px 5px;
        }

        table span {
            font-weight: normal;
        }

        table tr:last-child > td {
            border-bottom: 1px solid #004a8e;
        }

        table tr > td:last-child, table tr > th:last-child {
            border-right: 1px solid #004a8e;
        }

        table .table-header td {
            padding: 0;
            border-top: 0;
        }

        table td[rowspan="2"] {
            border-bottom: 1px solid #004a8e;
        }

        table .empty {
            height: 15px;
        }

        table .empty td:last-child {
            border-left: 0 !important;
            border-right: 0 !important;
        }

        .check {
            display: inline-block;
            width: 10px;
            height: 10px;
            outline: 2px solid grey;
            margin-left: 5px;
        }

        .check.active {
            border: 2px solid white;
            outline: 2px solid grey;
            background-color: black;
        }

        .offer__signature > div {
            width: 48%;
        }

        /* simple-table */

        table.simple-table tr:not(:first-child) td {
            border-top: 0 !important;
        }

        table.simple-table td {
            padding: 2px 5px;
        }

        table.simple-table tr td:first-child {
            width: 250px;
            padding-right: 30px;
        }

        table.simple-table tr td:nth-child(2) {
            width: 150px;
            border-left: 0;
        }

        table.simple-table tr td:nth-child(3) {
            width: 100px;
        }

        .cgv-page {
            page-break-before: always;
        }

        .other {
            page-break-inside: avoid;
        }

        table td.line-number {
            text-align: right;
        }

        .flex-end {
            justify-content: end;
            -webkit-box-pack: end;
        }

        /*.part-one-position {*/
        /*    position: relative;*/
        /*    left: 70%;*/
        /*}*/

        .not-breakable-block {
            page-break-inside: avoid;
        }
    </style>

</head>
<body>
<div class="offer__container">

    <div class="offer__title flex space-between">
        <h1>{{ 'Commercial.PDF.Title'|trans({}, "messages", app.request.locale) }}
            - {{ quoteRequest.reference }}</h1>
        <p class="offer__title__date">{{ 'now'|date('d/m/Y') }}</p>
    </div>

    <div class="offer__content">


        <div class="offer__ref">

            {# partie 1 #}

            <h2>1. {{ 'Commercial.PDF.CustomerTitle'|trans({}, "messages", app.request.locale)|upper }}</h2>
            <div class="flex">
                <div style="width: 60%;"></div>
                <div style="width: 40%;">
                    <p class="part-one-position">
                        <span class="bold">{{ 'Commercial.PDF.Company'|trans({}, "messages", app.request.locale) }} : </span> {{ quoteRequest.businessName }}
                    </p>
                    <p class="part-one-position">
                        <span class="bold">{{ 'Commercial.PDF.Address'|trans({}, "messages", app.request.locale) }} : </span> {{ quoteRequest.address }}
                    </p>
                    <p class="part-one-position">
                        <span class="bold">{{ 'Commercial.PDF.PostalCode'|trans({}, "messages", app.request.locale) }} : </span> {% if quoteRequest.postalCode %}{{ quoteRequest.postalCode.code }}{% endif %} {{ quoteRequest.city }}
                    </p>
                </div>
            </div>
            {# partie 2 #}

            <div>
                <h2>2. {{ 'Commercial.PDF.Summary'|trans({}, "messages", app.request.locale)|upper }}</h2>
                <div>
                    {% if quoteRequest.civility == 'M' %}
                    {{ 'Commercial.ConfirmEmail.Dear.M-long'|trans({}, "messages", locale) }},
                {% elseif quoteRequest.civility == 'MME' %}
                    {{ 'Commercial.ConfirmEmail.Dear.Mrs-long'|trans({}, "messages", locale) }},
                    {% endif %}<br><br>
                    Suite à votre demande, je vous prie de trouver ci-dessous notre proposition commerciale, valable 3
                    mois, concernant la gestion de vos déchets. Ce résumé regroupe l’ensemble des prestations souhaitées
                    et un commercial se tient à votre disposition pour préciser chacune des lignes.
                </div>
                <br>
                <div>
                    <span class="bold">{{ 'Commercial.PDF.OurIntervention'|trans({}, "messages", app.request.locale) }} : </span>
                    <div>
                        <ul>
                            <li>Mise à disposition du matériel de collecte sous 48h si besoin</li>
                            <li>Collecte des déchets stockés dans les contenants prévus à cet effet</li>
                            <li>Traitement et recyclage de vos déchets sur nos sites PAPREC, agrées et certifiés</li>
                            <li>Envoi d’un reporting mensuel</li>
                        </ul>
                    </div>
                </div>
                <br>
                <div>
                    <span class="bold">{{ 'Commercial.PDF.OurPrices'|trans({}, "messages", app.request.locale) }} : </span>
                    <br>
                    <br>
                    <div>
                        <table class="table table-striped table-bordered table-hover"
                               cellpadding="0" cellspacing="0" border="0" width="100%">
                            <thead>
                            <tr>
                                <th>{{ 'Commercial.QuoteRequestLine.ProductName'|trans }}</th>
                                <th style="width: 3%;">{{ 'Commercial.QuoteRequestLine.ProductBillingUnit'|trans }}</th>
                                <th style="width: 2%;">{{ 'Commercial.QuoteRequestLine.PDF.Quantity'|trans }}</th>
                                <th style="width: 8%;">{{ 'Commercial.QuoteRequestLine.PDF.PassingFrequency'|trans }}</th>
                                <th style="width: 8%;">PU<br>Location<br>(€/mois)</th>
                                <th style="width: 8%;">PU<br>Collecte<br>(€/unité)</th>
                                <th style="width: 11%;">PU<br>Traitement<br>(€/Tonne)</th>
                                <th style="width: 11%;">{{ 'Commercial.QuoteRequestLine.Package'|trans }}</th>
                            </tr>
                            </thead>
                            <tbody>
                            {% for quoteRequestLine in quoteRequest.quoteRequestLines %}
                                <tr style="page-break-inside: avoid;page-break-after: auto;">
                                    <td style="text-align: left;">
                                        {{ quoteRequestLine.product|productLabelTranslation(app.user.lang) }}
                                    </td>
                                    <td>
                                        {% if quoteRequestLine.product.billingUnit %}
                                            {{ quoteRequestLine.product.billingUnit.name }}
                                        {% endif %}
                                    </td>
                                    <td>
                                        {% if quoteRequestLine.product.calculationFormula == 'PACKAGE' %}
                                            {{ quoteRequestLine.quantity }}
                                        {% elseif quoteRequestLine.product.calculationFormula == 'UNIT_PRICE' %}
                                            {# RESTE NULL #}
                                        {% else %}
                                            {{ quoteRequestLine.quantity }}
                                        {% endif %}
                                    </td>
                                    <td>
                                        {% if quoteRequestLine.product.hideFrequency == 1 %}

                                        {% else %}
                                            {% if quoteRequestLine.frequency != 'REGULAR' %}
                                                {% if quoteRequestLine.frequency %}
                                                    {{ ('Commercial.QuoteRequestLine.Frequency-values.'~quoteRequestLine.frequency)|trans({}, "messages", app.request.locale) }}
                                                {% endif %}
                                            {% else %}
                                                {% if quoteRequestLine.frequencyInterval is not empty %}
                                                    {% set frequencyIntervalName = 'General.Frequency.' ~ quoteRequestLine.frequencyInterval %}
                                                    {{ quoteRequestLine.frequencyTimes }}
                                                    x/ {{ frequencyIntervalName|trans({}, "messages", app.request.locale) }}
                                                {% endif %}
                                            {% endif %}
                                        {% endif %}
                                    </td>
                                    <td class="line-number">
                                        {% if quoteRequestLine.product.calculationFormula == 'PACKAGE' %}
                                            {{ 0|formatAmount('EUR', app.request.locale) }}
                                        {% elseif quoteRequestLine.product.calculationFormula == 'UNIT_PRICE' %}
                                            {{ (quoteRequestLine|quoteRequestLineCalculatePrices('editableRentalUnitPrice'))|formatAmount('EUR', app.request.locale) }}
                                        {% else %}
                                            {{ (quoteRequestLine|quoteRequestLineCalculatePrices('editableRentalUnitPrice'))|formatAmount('EUR', app.request.locale) }}
                                        {% endif %}
                                    </td>
                                    <td class="line-number">
                                        {% if quoteRequestLine.product.calculationFormula == 'PACKAGE' %}
                                            {{ 0|formatAmount('EUR', app.request.locale) }}
                                        {% elseif quoteRequestLine.product.calculationFormula == 'UNIT_PRICE' %}
                                            {{ (quoteRequestLine|quoteRequestLineCalculatePrices('treatmentCollectPrice'))|formatAmount('EUR', app.request.locale) }}
                                        {% else %}
                                            {{ (quoteRequestLine|quoteRequestLineCalculatePrices('treatmentCollectPrice'))|formatAmount('EUR', app.request.locale) }}
                                        {% endif %}
                                    </td>
                                    <td class="line-number">
                                        {% if quoteRequestLine.editableTreatmentUnitPrice < 0 %}
                                            <i class="fas fa-exclamation-triangle" data-toggle="tooltip"
                                               data-placement="top"
                                               title="Le prix sera indexé mensuellement à la variation de la mercuriale Mercuriale choisie, référence Mois et Année de Référence"></i>
                                        {% endif %}
                                        {% if quoteRequestLine.product.calculationFormula == 'PACKAGE' %}
                                            {{ 0|formatAmount('EUR', app.request.locale) }}
                                        {% elseif quoteRequestLine.product.calculationFormula == 'UNIT_PRICE' %}
                                            {{ (quoteRequestLine|quoteRequestLineCalculatePrices('editableTreatmentUnitPrice'))|formatAmount('EUR', app.request.locale) }}
                                        {% else %}
                                            {{ (quoteRequestLine|quoteRequestLineCalculatePrices('editableTreatmentUnitPrice'))|formatAmount('EUR', app.request.locale) }}
                                        {% endif %}
                                    </td>
                                    <td class="line-number">
                                        {% if quoteRequestLine.product.calculationFormula == 'PACKAGE' or quoteRequestLine.product.catalog == 'MATERIAL' %}
                                            {{ (quoteRequestLine|quoteRequestLineCalculatePrices('totalAmount'))|formatAmount('EUR', app.request.locale) }}
                                        {% endif %}
                                    </td>
                                </tr>
                            {% endfor %}
                            {% if quoteRequest.managementFeeCode1 is not empty or quoteRequest.managementFeeDescription1 is not empty or quoteRequest.managementFeeFrequency1 is not empty or (quoteRequest.managementFeeAmount1 is not null and quoteRequest.managementFeeAmount1 >= 0) %}
                                <tr style="page-break-inside: avoid;page-break-after: auto;">
                                    <td style="text-align: left;">{{ quoteRequest.managementFeeDescription1 }}</td>
                                    <td></td>
                                    <td></td>
                                    <td>{% if quoteRequest.managementFeeFrequency1 %}{{ ('General.Frequency.' ~ quoteRequest.managementFeeFrequency1)|trans({}, "messages", app.request.locale) }}{% endif %}</td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td class="line-number">{{ quoteRequest.managementFeeAmount1|formatAmount('EUR', app.request.locale) }}</td>
                                </tr>
                            {% endif %}

                            </tbody>
                        </table>
                        {% if quoteRequest.managementFeeCode2 is not empty or quoteRequest.managementFeeDescription2 is not empty or quoteRequest.managementFeeFrequency2 is not empty or (quoteRequest.managementFeeAmount2 is not null and quoteRequest.managementFeeAmount2 >= 0) or quoteRequest.managementFeeCode3 is not empty or quoteRequest.managementFeeDescription3 is not empty or quoteRequest.managementFeeFrequency3 is not empty or (quoteRequest.managementFeeAmount3 is not null and quoteRequest.managementFeeAmount3 >= 0) or quoteRequest.managementFeeCode4 is not empty or quoteRequest.managementFeeDescription4 is not empty or quoteRequest.managementFeeFrequency4 is not empty or (quoteRequest.managementFeeAmount4 is not null and quoteRequest.managementFeeAmount4 >= 0) %}
                            <table class="table table-striped table-bordered table-hover"
                                   cellpadding="0" cellspacing="0" border="0" width="100%">
                                <thead>
                                <tr>
                                    <th style="text-align: left;">{{ 'Commercial.QuoteRequest.ManagementFeeDescription'|trans }}</th>
                                    <th style="width: 11%;">{{ 'Commercial.QuoteRequest.ManagementFeeFrequency'|trans }}</th>
                                    <th style="width: 11%;">{{ 'Commercial.QuoteRequest.ManagementFeeAmount'|trans }}</th>
                                </tr>
                                </thead>
                                <tbody>
                                {% if quoteRequest.managementFeeCode2 is not empty or quoteRequest.managementFeeDescription2 is not empty or quoteRequest.managementFeeFrequency2 is not empty or (quoteRequest.managementFeeAmount2 is not null and quoteRequest.managementFeeAmount2 >= 0) %}
                                    <tr>
                                        <td style="text-align: left;">{{ quoteRequest.managementFeeDescription2 }}</td>
                                        <td>{% if quoteRequest.managementFeeFrequency2 %}{{ ('General.Frequency.' ~ quoteRequest.managementFeeFrequency2)|trans({}, "messages", app.request.locale) }}{% endif %}</td>
                                        <td class="line-number">{{ quoteRequest.managementFeeAmount2|formatAmount('EUR', app.request.locale) }}</td>
                                    </tr>
                                {% endif %}
                                {% if quoteRequest.managementFeeCode3 is not empty or quoteRequest.managementFeeDescription3 is not empty or quoteRequest.managementFeeFrequency3 is not empty or (quoteRequest.managementFeeAmount3 is not null and quoteRequest.managementFeeAmount3 >= 0) %}
                                    <tr>
                                        <td style="text-align: left;">{{ quoteRequest.managementFeeDescription3 }}</td>
                                        <td>{% if quoteRequest.managementFeeFrequency3 %}{{ ('General.Frequency.' ~ quoteRequest.managementFeeFrequency3)|trans({}, "messages", app.request.locale) }}{% endif %}</td>
                                        <td class="line-number">{{ quoteRequest.managementFeeAmount3|formatAmount('EUR', app.request.locale) }}</td>
                                    </tr>
                                {% endif %}
                                {% if quoteRequest.managementFeeCode4 is not empty or quoteRequest.managementFeeDescription4 is not empty or quoteRequest.managementFeeFrequency4 is not empty or (quoteRequest.managementFeeAmount4 is not null and quoteRequest.managementFeeAmount4 >= 0) %}
                                    <tr>
                                        <td style="text-align: left;">{{ quoteRequest.managementFeeDescription4 }}</td>
                                        <td>{% if quoteRequest.managementFeeFrequency4 %}{{ ('General.Frequency.' ~ quoteRequest.managementFeeFrequency4)|trans({}, "messages", app.request.locale) }}{% endif %}</td>
                                        <td class="line-number">{{ quoteRequest.managementFeeAmount4|formatAmount('EUR', app.request.locale) }}</td>
                                    </tr>
                                {% endif %}
                                </tbody>
                            </table>
                        {% endif %}

                        {% if quoteRequest.catalog == 'REGULAR' or quoteRequest.catalog == 'regular' %}
                            <div>
                                <span class="bold">{{ 'Commercial.PDF.ContractDuration'|trans({}, "messages", app.request.locale) }} : </span>
                                <div style="margin-top: 5px">
                                    {% if quoteRequest.duration is not null %}
                                        {{ quoteRequest.duration }} {{ 'Commercial.QuoteRequest.MONTH'|trans }} à compter de la date de signature du présent contrat, ou de la date spécifiée au point 4.
                                    {% endif %}
                                </div>
                                {% if quoteRequest.startDate is not null %}
                                    <div>
                                        La date de démarrage des prestations est prévue pour
                                        le {{ quoteRequest.startDate|localizeddate('short', 'none', app.request.locale, app.user.timezone) }}
                                        .
                                    </div>
                                {% endif %}
                            </div>
                        {% elseif quoteRequest.catalog == 'PONCTUAL' or quoteRequest.catalog == 'ponctual' or quoteRequest.catalog == 'MATERIAL' or quoteRequest.catalog == 'material' %}
                            <div>
                                <span class="bold">{{ 'Commercial.PDF.PonctualDate'|trans({}, "messages", app.request.locale) }} : </span>
                                <div style="margin-top: 5px">
                                    {% if quoteRequest.ponctualDate is not null %}
                                        {{ quoteRequest.ponctualDate|localizeddate('short', 'none', app.request.locale, app.user.timezone) }}
                                    {% endif %}
                                </div>
                            </div>
                        {% endif %}
                    </div>
                </div>
            </div>

            {# partie 3 #}

            <div class="not-breakable-block">
                <h2>3. {{ 'Commercial.PDF.CommentTitle'|trans({}, "messages", app.request.locale)|upper }}</h2>
                <div>
                    {% if quoteRequest.comment or quoteRequest.interest %}
                        {% if quoteRequest.interest %}
                            {{ 'Commercial.PDF.SelectedInterestIs'|trans({}, "messages", app.request.locale) }} {{ ('Public.Contact.Interest-list.'~quoteRequest.interest)| trans({}, "messages", locale) }}.
                            <br>
                        {% endif %}
                        {% if quoteRequest.comment %}
                            {{ quoteRequest.comment }}
                        {% endif %}
                    {% else %}
                        {{ 'Commercial.PDF.NoComment'|trans({}, "messages", app.request.locale) }}
                    {% endif %}
                </div>
            </div>


            {# partie 4 #}

            <div class="not-breakable-block">
                <h2>4. {{ 'Commercial.PDF.Billing'|trans({}, "messages", app.request.locale)|upper }}</h2>
                <div>
                    La facturation sera adressée mensuellement en fonction des prix unitaires hors taxes précisés au
                    point 2 et de l’activité réelle du contrat ou si cela est spécifiée à l’article 4 des présentes
                    conditions particulières, sur la base d’un montant forfaitaire HT. La TVA sera appliquée aux
                    montants hors taxes au taux de TVA en vigueur.
                </div>
            </div>


            {# partie 5 #}
            <div class="not-breakable-block">

                <h2>5. {{ 'Commercial.PDF.ContactTitle'|trans({}, "messages", app.request.locale)|upper }}</h2>

                <div>
                    <div class="flex offer__ref-items">
                        <div>
                            <span class="bold">{{ 'Commercial.PDF.CustomerTitle'|trans({}, "messages", app.request.locale) }} : </span>
                        </div>
                        <div>
                        </div>
                    </div>
                    <div class="flex offer__ref-items">
                        <div>
                            <span class="">{{ 'Commercial.PDF.SignatoryLastName'|trans({}, "messages", app.request.locale) }} : </span> {{ quoteRequest.signatoryLastName1 }}
                        </div>

                        <div>
                            <span class="">{{ 'Commercial.PDF.ContactLastName'|trans({}, "messages", app.request.locale) }} : </span> {{ quoteRequest.lastName }}
                        </div>
                    </div>

                    <div class="flex offer__ref-items">
                        <div>
                            <span class="">{{ 'Commercial.PDF.SignatoryFirstName'|trans({}, "messages", app.request.locale) }} : </span> {{ quoteRequest.signatoryFirstName1 }}
                        </div>

                        <div>
                            <span class="">{{ 'Commercial.PDF.ContactFirstName'|trans({}, "messages", app.request.locale) }} : </span> {{ quoteRequest.firstName }}
                        </div>
                    </div>

                    <div class="flex offer__ref-items">
                        <div>
                            <span class="">{{ 'Commercial.PDF.SignatoryQuality'|trans({}, "messages", app.request.locale) }} : {{ ('General.' ~ quoteRequest.civility)|trans }}</span>
                        </div>
                        <div>
                            <span class="">{{ 'Commercial.PDF.PhoneNumber'|trans({}, "messages", app.request.locale) }} : </span> {{ quoteRequest.phone }}
                        </div>
                    </div>

                    <div class="flex offer__ref-items">
                        <div></div>
                        <div>
                            <span class="">{{ 'Commercial.PDF.Email'|trans({}, "messages", app.request.locale) }} : </span> {{ quoteRequest.email }}
                        </div>
                    </div>
                </div>
                <div>
                    <div class="flex offer__ref-items">
                        <div>
                            <span class="bold">{{ 'Commercial.PDF.CommercialTitle'|trans({}, "messages", app.request.locale) }} : </span>
                        </div>
                        <div></div>
                    </div>
                    <div class="flex offer__ref-items">
                        <div>
                            <span class="">{{ 'Commercial.PDF.LastName'|trans({}, "messages", app.request.locale) }} : </span> {% if quoteRequest.userInCharge is not null %}{{ quoteRequest.userInCharge.lastName }}{% endif %}
                        </div>
                        <div></div>
                    </div>

                    <div class="flex offer__ref-items">
                        <div>
                            <span class="">{{ 'Commercial.PDF.FirstName'|trans({}, "messages", app.request.locale) }} : </span> {% if quoteRequest.userInCharge is not null %}{{ quoteRequest.userInCharge.firstName }}{% endif %}
                        </div>
                        <div></div>
                    </div>

                    <div class="flex offer__ref-items">
                        <div>
                            <span class="">{{ 'Commercial.PDF.PhoneNumber'|trans({}, "messages", app.request.locale) }} : {% if quoteRequest.userInCharge is not null %}{{ quoteRequest.userInCharge.phoneNumber }}{% endif %}</span>
                        </div>
                        <div></div>
                    </div>

                    <div class="flex offer__ref-items">
                        <div>
                            <span class="">{{ 'Commercial.PDF.Email'|trans({}, "messages", app.request.locale) }} : </span> {% if quoteRequest.userInCharge is not null %}{{ quoteRequest.userInCharge.email }}{% endif %}
                        </div>
                        <div></div>
                    </div>
                </div>
            </div>

